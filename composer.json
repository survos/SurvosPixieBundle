{
    "name": "survos/pixie-bundle",
    "description": "Symfony Bundle allow allow managing museum data in an independent SQLite database using a doctrine ORM",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "ext-pdo": "*",
        "php": "^8.4",
        "symfony/config": "^7.4||^8.0",
        "symfony/dependency-injection": "^7.4||^8.0",
        "symfony/http-kernel": "^7.4||^8.0",
        "twig/twig": "^3.4",
        "symfony/yaml": "^7.4||^8.0",
        "doctrine/dbal": "^4.3.2",
        "symfony/property-access": "^7.4||^8.0",
        "symfony/twig-bundle": "^7.4||^8.0",
        "symfony/routing": "^7.4||^8.0",
        "symfony/serializer": "^7.4||^8.0",
        "symfony/serializer-pack": "^1.3",
        "symfony/ux-icons": "^2.21",
        "symfony/http-client-contracts": "^3.5",
        "symfony/http-client": "^7.4||^8.0",
        "symfony/asset": "^7.4||^8.0",
        "illuminate/collections": "^12",
        "symfony/object-mapper": "^7.4||^8.0",
        "symfony/console": "^7.4||^8.0",
        "survos/babel-bundle": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.4||^8.0",
        "symfony/framework-bundle": "^7.4||^8.0",
        "symfony/var-dumper": "^7.4||^8.0",
        "phpunit/phpunit": "^12",
        "symfony/phpunit-bridge": "^7.4||^8.0",
        "symfony/stopwatch": "^7.4||^8.0",
        "survos/state-bundle": "^2.0",
        "symfony/ux-chartjs": "^2.21",
        "survos/bootstrap-bundle": "^2.0",
        "symfony/event-dispatcher": "^7.4||^8.0",
        "symfony/messenger": "^7.4||^8.0",
        "survos/bunny-bundle": "^2.0",
        "zenstruck/filesystem": "1.x-dev",
        "league/flysystem-ziparchive": "^3.29",
        "league/csv": "^9.9",
        "psr/log": "^3.0",
        "survos/scraper-bundle": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.65",
        "meilisearch/meilisearch-php": "^1.11",
        "symfony/security-bundle": "^7.4||^8.0",
        "rector/rector": "^2.0",
        "doctrine/orm": "^2.12 || ^3.3",
        "survos/sais-bundle": "^2.0",
        "api-platform/core": "^4.0",
        "survos/meili-bundle": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\PixieBundle\\": "src/",
            "Survos\\PixieBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "survos/installer": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
